{
  "expo": {
    "name": "WAgeni",
    "slug": "wageni",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#f8fafc"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wageni.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#16a34a"
      },
      "package": "com.wageni.app"
    },
    "web": {},
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "scheme": "wageni"
  }
}
